{
	"expo": {
		"name": "coss",
		"slug": "coss",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./public/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./public/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./public/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.hitosea.coss"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./public/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
